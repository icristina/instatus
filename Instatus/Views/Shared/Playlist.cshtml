@model WebSection

<section>
    @Html.Tag("h1", Model.Heading)
    @Html.Tag("h2", Model.SubHeading)
    <ul>
        @foreach (var link in Model.Links)
        {
            <li>
                @Html.Tag("b", link.Title)
                @Html.Anchor("View", WebPaths.Absolute(link.Uri))
            </li>
        }
    </ul>
</section>